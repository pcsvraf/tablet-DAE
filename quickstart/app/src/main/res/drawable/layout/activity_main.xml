<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.servicio.calidad.encuesta.encuestatuopiniondf.PrincipalActivity"
    android:orientation="vertical">
    <LinearLayout
        android:background="@drawable/encuesta"
        android:id="@+id/l1"
        android:layout_margin="10dp"
        android:layout_width="1260dp"
        android:layout_height="700dp"
        android:orientation="vertical">


    </LinearLayout>

    <LinearLayout
        android:id="@+id/l2"
        android:layout_width="1270dp"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_marginTop="100dp"
        android:background="@color/blanco"
        android:orientation="horizontal">


        <Button
            android:id="@+id/buttonsub"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="431dp"
            android:layout_marginStart="550dp"
            android:background="@drawable/buttonstyle"
            android:text="Iniciar encuesta"
            android:textColor="@color/blanco" />
    </LinearLayout>
</LinearLayout>
